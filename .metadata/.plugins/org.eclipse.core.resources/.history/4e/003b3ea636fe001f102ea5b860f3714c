package basicNumbers;

import java.util.Iterator;

public class Floyds_triangle {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int n = 5;
		int k =1;
		for (int i = 1; i <=n; i++) {
			for (int j = 1; j  <=i ; j++) {
				
				for (int k = 1; k <= j; k++) {
					System.out.print(" ");
				}
				
				System.out.print(j);
				
			}
			
			}
			System.out.println();
		}
		
		
		

}
