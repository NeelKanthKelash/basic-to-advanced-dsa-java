package basicNumbers;

public class Power_Using_loop {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a = 3;
		int b = 4;
		int count = 0;
		int pow = 0;
		
		while (b<0) {
			 pow = a*b;
			 a=b;
			 b=pow;
			++count;
			System.out.println("pow "+ pow);
		}
		
		
	}

}
