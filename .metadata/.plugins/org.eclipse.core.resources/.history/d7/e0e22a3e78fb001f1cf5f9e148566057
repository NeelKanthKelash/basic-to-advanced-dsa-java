package basicNumbers;

import java.util.Scanner;

public class Number_Count {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int count = 0;
		int sum = 0;
		while (n != 0) {
			int reminder = n%10;
			 sum = sum*10+reminder;
			
		}
		System.out.println(n);

	}
}
