package com.practice.problems;

import java.util.LinkedList;

public class Linked_List {

	public static void main(String[] args) {
		LinkedList<String>list = new LinkedList<String>();
		list.add("is");
		list.add("a");
		list.addLast("list");
		list.addFirst("this");
		list.add(3,"linked");
		System.out.println(list);

	}

}
