package basicNumbers;

import java.util.Scanner;

public class Average_of_3Numbers {

	// functions creating 
	public static float averageOfThreeNumbers(float average ) {
	
		float a = 0,b = 0,c = 0;
		
		float avg = (a+ b+ c)/3;
		return avg ;
		
	}
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();

		int average = a+b+c/3;
		averageOfThreeNumbers(average);
	}


}
