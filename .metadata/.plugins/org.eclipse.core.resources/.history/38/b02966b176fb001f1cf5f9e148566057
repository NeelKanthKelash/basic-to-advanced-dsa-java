package basicNumbers;

import java.util.Scanner;

public class Number_Count {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int count = 0;
		int sum = 0;
		while (n != 0) {
			n /= 10;
			sum = n+count;
			++count;
			
		}
		System.out.println(sum);

		
	}
}
